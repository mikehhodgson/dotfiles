{
  "editor.bracketPairColorization.enabled": true,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "[json][jsonc]": {
    "editor.codeActionsOnSave": ["source.fixAll"]
  },
  "editor.inlayHints.enabled": "offUnlessPressed",
  "remote.extensionKind": {
    "lfs.vscode-emacs-friendly": ["ui"],
    "msjsdiag.debugger-for-chrome": ["workspace"]
  },
  "xmlTools.splitXmlnsOnFormat": false,
  "xmlTools.splitAttributesOnFormat": true,
  "files.associations": {
    "Vagrantfile": "ruby",
    "*.log_*": "log"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.settings": true,
    "**/.classpath": true,
    "**/.project": true
    //    "**/target": true
  },
  "workbench.settings.editor": "json",
  "workbench.tree.renderIndentGuides": "always",
  "workbench.tree.indent": 16,
  "window.restoreWindows": "none",
  "window.titleBarStyle": "custom",
  "window.enableMenuBarMnemonics": false,
  "window.openWithoutArgumentsInNewWindow": "on",
  "window.openFilesInNewWindow": "on",
  "window.openFoldersInNewWindow": "on",
  "workbench.iconTheme": "seti",
  "workbench.tips.enabled": false,
  "npm.enableScriptExplorer": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.decorations.enabled": true,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.codeLens": true,
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "block",
  "editor.fontFamily": " Hack, 'Comic Mono', 'GohuFont 11 Nerd Font Mono', 'Terminess Nerd Font Mono', Consolas, 'Courier New', monospace",
  "editor.formatOnSave": true,
  "editor.minimap.maxColumn": 80,
  "editor.minimap.showSlider": "mouseover",
  "editor.minimap.renderCharacters": false,
  "editor.multiCursorPaste": "full",
  "editor.suggest.showWords": false,
  "editor.quickSuggestions": {
    "comments": false,
    "strings": true,
    "other": true
  },
  "editor.rulers": [80],
  "editor.scrollbar.verticalScrollbarSize": 20,
  "editor.smoothScrolling": false,
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.overviewRulerBorder": false,
  "editor.snippetSuggestions": "inline",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
  "explorer.confirmDragAndDrop": false,
  "powershell.powerShellDefaultVersion": "PowerShell (x64)",
  "problems.showCurrentInStatus": true,
  "terminal.integrated.minimumContrastRatio": 4.5,
  //"editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 80,
  "[dockerfile]": {
    "editor.tabSize": 4
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": false
  },
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.formatOnSave": false
  },
  /*   "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "javascriptreact"
  }, */
